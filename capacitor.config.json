{
  "appId": "fairduty.easyfriend",
  "appName": "FairDuty",
  "webDir": "build",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "launchAutoHide": true, 
      "backgroundColor": "#ffffff", 
      "showSpinner": true, 
      "androidSpinnerStyle": "large",
      "iosSpinnerStyle": "small",
      "spinnerColor": "#999999",
      "splashFullScreen": true,   
      "splashImmersive": true,   
      "useDialog": false 
    }
  }
}
